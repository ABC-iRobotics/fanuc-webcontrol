PROGRAM webstop
%COMMENT = 'v2'
%NOLOCKGROUP
%NOPAUSE=ERROR+COMMAND+TPENABLE
%NOBUSYLAMP

CONST

	R_WEBKEEP = 42

VAR

	STATUS: INTEGER
	return_code: INTEGER

BEGIN

	SET_REAL_REG(R_WEBKEEP, 99, STATUS)
	
	IF (STATUS <> 0 ) THEN
	
		WRITE ('SET_REAL_REG  built-IN failed WITH STATUS = ', STATUS, CR)
	
	ELSE

		return_code = 204
	
	ENDIF
	
END webstop