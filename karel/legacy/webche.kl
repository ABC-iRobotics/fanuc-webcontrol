				IF (STATUS <> 0 ) THEN
					WRITE ('WEBCHECK R_WEBCHECK set is  failed: ', STATUS, CR)
				ENDIF
				limit_j3 = 3
			ENDIF
		ENDIF
	ENDIF

	IF UNINIT( val_new_jpos[4] ) THEN 
		val_new_jpos[4] = 0
	ELSE	
