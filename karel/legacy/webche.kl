				IF (STATUS <> 0 ) THEN
					WRITE ('WEBCHECK R_WEBCHECK set is  failed: ', STATUS, CR)
				ENDIF
				limit_j4 = 6
			ENDIF
		ELSE
			IF val_new_jpos[4] <= j4min_int THEN
				SET_REAL_REG(R_WEBCHECK, 0, STATUS)
				IF (STATUS <> 0 ) THEN
					WRITE ('WEBCHECK R_WEBCHECK set is  failed: ', STATUS, CR)
				ENDIF
				limit_j4 = 6
			ENDIF
		ENDIF	
		IF j4max_flag THEN
			IF val_new_jpos[4] >= j4max_real THEN
				SET_REAL_REG(R_WEBCHECK, 0, STATUS)
				IF (STATUS <> 0 ) THEN
					WRITE ('WEBCHECK R_WEBCHECK set is  failed: ', STATUS, CR)
				ENDIF
				limit_j4 = 3
			ENDIF
		ELSE
			IF val_new_jpos[4] >= j4max_int THEN
				SET_REAL_REG(R_WEBCHECK, 0, STATUS)
				IF (STATUS <> 0 ) THEN
					WRITE ('WEBCHECK R_WEBCHECK set is  failed: ', STATUS, CR)
				ENDIF
				limit_j4 = 3
			ENDIF
		ENDIF
	ENDIF

	IF UNINIT( val_new_jpos[5] ) THEN 
		val_new_jpos[5] = 0
	ELSE	
		IF j5min_flag THEN
			IF val_new_jpos[5] <= j5min_real THEN
				SET_REAL_REG(R_WEBCHECK, 0, STATUS)
				IF (STATUS <> 0 ) THEN
					WRITE ('WEBCHECK R_WEBCHECK set is  failed: ', STATUS, CR)
				ENDIF
